<br /><br /><br /><br /><br /><br />

<main class="main" id="main" role="main">

  <% if in_lightbox? %>
    <div class="page-overlay"></div>
    <article role="article" class="detail-view detail-view--event" tabindex="-1" role="dialog" aria-labelledby="detail-view__hdr" aria-hidden="true">
  <% else %>
    <article role="article" class="detail-view detail-view--event">
  <% end %>
    <%= render "shared/detail_view/controls" %>
    <%= yield %>
  </article>

</main>

<%= parent_layout 'application' %>